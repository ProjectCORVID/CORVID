Referent = require 'referent'

module.exports =
  class Relation extends Referent
    constructor: ({@subj, @rel, @obj, @params}) ->
      super
      @db.registerRel this

    relate: (subject, object, @params = {}) ->
      new Relation
        @rel = this
        @sub = subject
        @obj = object

    toString: ->
      "#{@subj.name} #{} #{@obj.name}"


